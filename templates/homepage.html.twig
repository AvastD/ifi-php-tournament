{%  extends 'base.html.twig' %}

{% block title %}Tournament{% endblock %}

{% block body %}
    <h1>Tournament Management</h1>

    <p>{{ message }}</p>

    <ul>
        {% for tournament in tournaments %}
            <li>{{ tournament.name }}</li>
        {% else %}
                <li>No tournaments</li>
        {% endfor %}
    </ul>
{% endblock %}